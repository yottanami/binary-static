  %div.grd-grid-12.grd-parent
    %h1= "Forms"
    %div.grd-grid-6
      %pre
        <fieldset>
          <legend>Fieldset Legend</legend>
          <div class="grd-row-padding">
            <div class='grd-grid-4 form_label'>
              <label for='loginid'>Login ID</label>
            </div>
            <div class='grd-grid-8'>
              <input type='text' name='loginid' />
              <p class='hint'>Hint comes here</p>
              <p class='errorfield'>error in loginid</p>
            </div>
          </div>
          <div class="grd-row-padding">
            <div class='grd-grid-4'></div>
            <div class='grd-grid-8'>
              <span class='button'>
                <button class='button' name='submit' type='submit'>Login</button>
              </span>
            </div>
          </div>
        </fieldset>
    %div.grd-grid-6
      %fieldset
        %legend= 'Fieldset Legend'
          %div.grd-row-padding
            .grd-grid-4.form_label
              %label{:for => "loginid"}= "Login ID"
            .grd-grid-8
              %input#loginid{:type => 'text', :name => 'loginid'}
              %p.hint= "Hint comes here"
              %p.errorfield= "error in loginid"
          %div.grd-row-padding
            .grd-grid-4
            .grd-grid-8
              %span.button
                %button#submit.button{:name => "submit" :type => "submit"}= "Login"

  %div.grd-grid-12.grd-parent
    %h1= "Inpage Popup"
    %div.grd-grid-6
      %pre
        <a href="#" class="has-inpage-popup"
        popup-content="popup contents">
          Open Inpage Popup
        </a>
    %div.grd-grid-6
      %div
        %a.has-inpage-popup{:href => "#" :popup-content => "popup contents"}= "Open Inpage Popup"
    .grd-clear
    %div.grd-grid-6
      %pre
        <span class='button'>
          <button id="_open-popup" class="has-inpage-popup"
          popup-width="800">
            Open Inpage Popup
          </button>
        </span>
        <div class="inpage_popup_content invisible" id="_open-popup-content">
          <h2>Popup Title</h2>
          <p>contents of the popup, might contain HTML.</p>
          <div class="drag-handle inpage_popup_content_box">
            <p>if you want to drag the popup, use this section</p>
          </div>
        </div>
    %div.grd-grid-6
      %div
        %span.button
          %button.has-inpage-popup{:id => "_open-popup" :popup-width => "800"}= "Open Inpage Popup"
        %div.inpage_popup_content{:class => 'invisible' :id => "_open-popup-content"}
          %h2= "Popup Title"
          %p
            contents of the popup, might contain HTML.
          %div.drag-handle{ :class => "inpage_popup_content_box"}
            %p if you want to drag the popup, use this section.

  %div.grd-grid-12.grd-parent
    %h1= "Date/Time Picker"
    %div.grd-grid-6
      %pre
        <label for="date">Date</label>
        <input class="has-date-picker" name="date"/>
    %div.grd-grid-6
      %label{:for => 'date'}= 'Date'
      %input.has-date-picker{:name => 'date'}
    .grd-clear
    %div.grd-grid-6
      %pre
        <label for="time">Time</label>
        <input class="has-time-picker"
        name="time" :time:minTimeHour="8" />
    %div.grd-grid-6
      %label{:for => 'time'}= 'Time'
      %input.has-time-picker{:name => 'time' :time:minTimeHour => "8"}
